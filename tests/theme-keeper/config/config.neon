extensions:
	themes: ThemeKeeper\DI\ThemeKeeperExtension

themes:
	admin:
		themeDir: theme/<themeName>
		assetsDir: test
		views:
			controls: <themeDir>/controls/<name>/<view>
			presenters: <themeDir>/presenters/<name>/<view>
			layouts: <themeDir>/@<view>

	web:
		views:
			controls: <themeDir>/<themeName>/controls/<name>/<view>
			presenters: <themeDir>/<themeName>/presenters/<name>/<view>
			layouts: <themeDir>/<themeName>/@<view>
